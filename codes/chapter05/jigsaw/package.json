{
  "name": "frontend-tutorial",
  "version": "1.0.0",
  "description": "frontend-tutorial 是一个从零开始学习 react 的教程。",
  "scripts": {
    "clean": "rimraf ./dist npm-debug.log*",
    "dev": "webpack-dev-server --config webpack.config.dev.js --color",
    "prod": "webpack --config webpack.config.prod.js",
    "start": "if-env NODE_ENV=production && npm run prod || npm run dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dunwu/frontend-tutorial"
  },
  "keywords": [
    "react",
    "npm",
    "webpack",
    "babel",
    "react-router"
  ],
  "author": "Zhang Peng",
  "license": "MIT",
  "dependencies": {
    "react": "15.x",
    "react-dom": "15.x",
    "react-router": "4.x",
    "react-router-dom": "4.x"
  },
  "devDependencies": {
    "babel-core": "6.x",
    "babel-loader": "7.x",
    "babel-plugin-syntax-dynamic-import": "6.x",
    "babel-preset-es2015": "6.x",
    "babel-preset-react": "6.x",
    "babel-preset-stage-0": "6.x",
    "css-loader": "0.x",
    "extract-text-webpack-plugin": "2.x",
    "file-loader": "0.x",
    "html-webpack-plugin": "2.x",
    "if-env": "1.x",
    "image-webpack-loader": "3.x",
    "moment": "2.x",
    "open-browser-webpack-plugin": "0.x",
    "react-hot-loader": "^3.0.0-beta.7",
    "rimraf": "2.x",
    "style-loader": "0.x",
    "webpack": "2.x",
    "webpack-dev-server": "2.x",
    "webpack-merge": "4.x"
  }
}
